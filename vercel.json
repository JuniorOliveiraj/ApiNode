{
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "crons": [
        {
            "path": "/mirante/list/cupons/atualizar",
            "schedule": "30 14 * * *"
        },
        {
            "path": "/mirante/list/cupons/atualizar",
            "schedule": "* * * * *"
        }
        
    ]
}