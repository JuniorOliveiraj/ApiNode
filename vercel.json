{
    "version": 2,
    "builds": [
        {
            "src": "/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "crons": [
        {
            "path": "/mirante/list/cupons/atualizar",
            "schedule": "00 06 * * *"
            
        },
        {
            "path": "/mirante/list/cupons/atualizar",
            "schedule": "00 12 * * *"
            
        },
        {
            "path": "/mirante/list/cupons/atualizar",
            "schedule": "00 00 * * *"
            
        }
        

    ]
}